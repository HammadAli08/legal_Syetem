{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key",
    "HUGGINGFACE_API_KEY": "@huggingface_api_key",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_COLLECTION": "legal_precedents"
  }
}
